    "ts:check": "tsc --noEmit",
    "test:unit": "vitest",
    "coverage": "vitest --coverage",
    "process": "tsx ./process-breeds/process.ts",
    "download": "tsx ./process-breeds/download.ts",
    "prod": "node ./backend/server.js"
